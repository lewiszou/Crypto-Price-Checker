{
    "manifest_version": 2,
    "name": "Crypto Price Checker",
    "description": "Check crypto prices!",
    "version": "1.0.5",
    "icons": {"128": "icon_128.png"},
    "permissions": [
        "...",
        "https://www.coingecko.com/en/widgets/"
      ],
    "background": 
    {
        "script": ["https://code.jquery.com/jquery-3.3.1.min.js", 
        "https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js",
        "https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js",
        "https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js",
        "https://www.coingecko.com/en/widgets/"]
    },
    "content_security_policy": "script-src 'self' https://www.coingecko.com/en/widgets/; object-src 'self'",
    "browser_action": 
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }


}